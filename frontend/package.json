{
    "name": "aquatechteamsbot",
    "version": "0.1.0",
    "engines": {
        "node": "18 || 20 || 22"
    },
    "private": true,
    "main": "./lib/app.js",
    "dependencies": {
        "express": "^4.21.1",
        "mkcert": "^3.2.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "send": "^0.18.0"
    },
    "devDependencies": {
        "@microsoft/teams-js": "^2.31.1",
        "@types/express": "^5.0.0",
        "@types/node": "^18.0.0",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "@types/send": "^0.17.1",
        "@vitejs/plugin-react": "^4.7.0",
        "env-cmd": "^10.1.0",
        "nodemon": "^3.1.7",
        "shx": "^0.3.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.1.2",
        "vite": "^6.3.5",
        "vite-plugin-mkcert": "^1.17.8"
    },
    "scripts": {
        "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run start",
        "start": "nodemon",
        "dev": "vite",
        "prestart": "npm run build:frontend",
        "build": "npm run build:frontend && npm run build:backend",
        "build:frontend": "vite build",
        "build:backend": "tsc --build && shx cp -r ./src/views ./src/static ./lib/",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "homepage": "."
}
